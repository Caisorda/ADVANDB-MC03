<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mc03.controller.QueryListController">
   <children>
      <ListView fx:id="queryListView" layoutX="11.0" layoutY="250.0" prefHeight="237.0" prefWidth="307.0" />
      <Button fx:id="cropVolume" layoutX="11.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleCropVolumeButton" prefHeight="25.0" prefWidth="130.0" text="Crop Volume" />
      <Button fx:id="numHouseholds" layoutX="215.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleNumberHouseholds" text="Crop per Household" />
      <Button fx:id="hasInsurance" layoutX="15.0" layoutY="162.0" mnemonicParsing="false" onAction="#handleHasInsurance" prefHeight="25.0" prefWidth="191.0" text="Households that have Insurance" />
      <Button fx:id="totalIncomeInsurance" layoutX="14.0" layoutY="96.0" mnemonicParsing="false" onAction="#totalIncomeHasInsurance" text="Total Income that have Insurance" />
      <Button fx:id="totalFarmland" layoutX="215.0" layoutY="54.0" mnemonicParsing="false" onAction="#totalFarmlandArea" prefHeight="25.0" prefWidth="124.0" text="Total Farmland" />
      <Button fx:id="numHouseholdsCropType" layoutX="15.0" layoutY="130.0" mnemonicParsing="false" onAction="#handlenumHouseholdsCropType" prefHeight="25.0" prefWidth="192.0" text="number of Households crop type" />
      <Button fx:id="cropVolumeCropType" layoutX="11.0" layoutY="54.0" mnemonicParsing="false" onAction="#handlecropVolumeCropType" text="crop volume per type" />
      <Button fx:id="listQueries" layoutX="207.0" layoutY="209.0" mnemonicParsing="false" onAction="#handleListQueries" text="Add List Of Queries" />
      <Button fx:id="updateButton" layoutX="260.0" layoutY="130.0" mnemonicParsing="false" onAction="#handleUpdateButton" text="Update?" />
   </children>
</AnchorPane>
