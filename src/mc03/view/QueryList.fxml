      <Button fx:id="totalFarmland" layoutX="96.0" layoutY="198.0" mnemonicParsing="false" onAction="#totalFarmlandArea" text="Total Farmland" />
      <Button fx:id="numHouseholdsCropType" layoutX="49.0" layoutY="128.0" mnemonicParsing="false" onAction="#handlenumHouseholdsCropType" text="number of Households crop type" />
